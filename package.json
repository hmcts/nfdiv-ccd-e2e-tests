  "private": true,
  "description": "",
  "scripts": {
    "test:test-e2e-standalone": "SHOW_BROWSER_WINDOW=yes NODE_PATH=. E2E_AUTO_DELAY_ENABLED=true node ./node_modules/codeceptjs/bin/codecept.js run tests/nfdiv/solicitorCreateSoleDivorceApplication.test.js  -c ./ --steps",
    "test:e2e-all": "SHOW_BROWSER_WINDOW=yes NODE_PATH=. E2E_AUTO_DELAY_ENABLED=true node ./node_modules/codeceptjs/bin/codecept.js run tests/nfdiv/nightly/*.test.js  -c ./ --steps",
    "pretest": "npm run create:type-definitions",
    "test:fullfunctional": "TESTS_FOR_ACCESSIBILITY='true' codeceptjs run-multiple parallel --steps --reporter mochawesome",
    "test:gen-order": "SHOW_BROWSER_WINDOW=yes NODE_PATH=. E2E_AUTO_DELAY_ENABLED=true node ./node_modules/codeceptjs/bin/codecept.js run tests/nfdiv/nightly/generalOrderReferralAndEmail.test.js  -c ./ --steps",
    "test:verify-state": "SHOW_BROWSER_WINDOW=no NODE_PATH=. E2E_AUTO_DELAY_ENABLED=true node ./node_modules/codeceptjs/bin/codecept.js run tests/nfdiv/nightly/verifyCaseStateAndEvents.test.js  -c ./ --steps",
    "test:draft-case": "SHOW_BROWSER_WINDOW=no NODE_PATH=. E2E_AUTO_DELAY_ENABLED=true node ./node_modules/codeceptjs/bin/codecept.js run tests/nfdiv/nightly/createDraftCase.test.js -c ./ --steps",
    "test:test-pba": "SHOW_BROWSER_WINDOW=yes NODE_PATH=. E2E_AUTO_DELAY_ENABLED=true node ./node_modules/codeceptjs/bin/codecept.js run tests/nfdiv/nightly/submitCaseWithPbaPayment.test.js  -c ./ --steps",
    "test:draft-aos": "SHOW_BROWSER_WINDOW=yes NODE_PATH=. E2E_AUTO_DELAY_ENABLED=true node ./node_modules/codeceptjs/bin/codecept.js run tests/nfdiv/nightly/draftAoSUpdateAndSubmit.test.js  -c ./ --steps",
    "test:add-note": "SHOW_BROWSER_WINDOW=yes NODE_PATH=. E2E_AUTO_DELAY_ENABLED=true node ./node_modules/codeceptjs/bin/codecept.js run tests/nfdiv/nightly/addNoteAndUpdateCase.test.js  -c ./ --steps",
    "test:hwf-refused": "SHOW_BROWSER_WINDOW=yes NODE_PATH=. E2E_AUTO_DELAY_ENABLED=true node ./node_modules/codeceptjs/bin/codecept.js run tests/nfdiv/nightly/hwfRefusedByCaseWorker.test.js -c ./ --steps",
    "test:create-sole": "SHOW_BROWSER_WINDOW=yes NODE_PATH=. E2E_AUTO_DELAY_ENABLED=true node ./node_modules/codeceptjs/bin/codecept.js run tests/nfdiv/nightly/solicitorCreateSoleDivorceApplication.test.js -c ./ --steps",
    "test:court-service": "SHOW_BROWSER_WINDOW=yes NODE_PATH=. E2E_AUTO_DELAY_ENABLED=true node ./node_modules/codeceptjs/bin/codecept.js run tests/nfdiv/nightly/verifyCaseForCourtService.test.js -c ./ --steps",
    "test:confirm-service": "SHOW_BROWSER_WINDOW=yes NODE_PATH=. E2E_AUTO_DELAY_ENABLED=true node ./node_modules/codeceptjs/bin/codecept.js run tests/nfdiv/nightly/verifyConfirmService.test.js -c ./ --steps",
    "test:CW-withdraw": "SHOW_BROWSER_WINDOW=yes NODE_PATH=. E2E_AUTO_DELAY_ENABLED=true node ./node_modules/codeceptjs/bin/codecept.js run tests/nfdiv/nightly/verifyWithdrawn.test.js -c ./ --steps",
    "test:share-case": "SHOW_BROWSER_WINDOW=yes NODE_PATH=. E2E_AUTO_DELAY_ENABLED=true node ./node_modules/codeceptjs/bin/codecept.js run tests/nfdiv/nightly/shareACaseAndDraftUpdateSubmitAoS.test.js -c ./ --steps",
    "create:type-definitions": "codeceptjs def",
    "lint": "eslint .",
    "test-crossbrowser-e2e": "NODE_PATH=. codeceptjs run-multiple ${BROWSER_GROUP:-'--all'} -c saucelabs.conf.js --plugins retryFailedStep --steps --grep '@crossbrowser' --reporter mocha-multi",
    "test:crossbrowser": "./bin/run-crossbrowser-tests.sh"


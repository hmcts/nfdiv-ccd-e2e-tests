{
  "name": "nfdiv-ccd-e2e-tests",
  "version": "0.0.0",
  "private": true,
  "description": "",
  "scripts": {
    "test:test-e2e-standalone": "SHOW_BROWSER_WINDOW=yes NODE_PATH=. E2E_AUTO_DELAY_ENABLED=true node ./node_modules/codeceptjs/bin/codecept.js run tests/nfdiv/solicitorCreateSoleDivorceApplication.test.js  -c ./ --steps",
    "pretest": "npm run create:type-definitions",
    "test:fullfunctional": "TESTS_FOR_ACCESSIBILITY='true' codeceptjs run-multiple parallel --steps --reporter mochawesome",
    "test:test-gen-order": "SHOW_BROWSER_WINDOW=yes NODE_PATH=. E2E_AUTO_DELAY_ENABLED=true node ./node_modules/codeceptjs/bin/codecept.js run tests/nfdiv/generalOrderReferralAndEmail.test.js  -c ./ --steps",
    "test:verify-state": "SHOW_BROWSER_WINDOW=no NODE_PATH=. E2E_AUTO_DELAY_ENABLED=true node ./node_modules/codeceptjs/bin/codecept.js run tests/nfdiv/verifyCaseStateAndEvents.test.js  -c ./ --steps",
    "test:test-shareACase": "SHOW_BROWSER_WINDOW=yes NODE_PATH=. E2E_AUTO_DELAY_ENABLED=true node ./node_modules/codeceptjs/bin/codecept.js run tests/nfdiv/shareACaseAndMoveToHolding.test.js  -c ./ --steps",
    "test:draft-case": "SHOW_BROWSER_WINDOW=no NODE_PATH=. E2E_AUTO_DELAY_ENABLED=true node ./node_modules/codeceptjs/bin/codecept.js run tests/nfdiv/createDraftCase.test.js -c ./ --steps",
    "test:test-pba": "SHOW_BROWSER_WINDOW=yes NODE_PATH=. E2E_AUTO_DELAY_ENABLED=true node ./node_modules/codeceptjs/bin/codecept.js run tests/nfdiv/submitCaseWithPbaPayment.test.js  -c ./ --steps",
    "test:draft-aos": "SHOW_BROWSER_WINDOW=yes NODE_PATH=. E2E_AUTO_DELAY_ENABLED=true node ./node_modules/codeceptjs/bin/codecept.js run tests/nfdiv/draftAoSUpdateAndSubmit.test.js  -c ./ --steps",
    "test:add-note": "SHOW_BROWSER_WINDOW=yes NODE_PATH=. E2E_AUTO_DELAY_ENABLED=true node ./node_modules/codeceptjs/bin/codecept.js run tests/nfdiv/addNoteAndUpdateCase.test.js  -c ./ --steps",
    "test:hwf-refused": "SHOW_BROWSER_WINDOW=yes NODE_PATH=. E2E_AUTO_DELAY_ENABLED=true node ./node_modules/codeceptjs/bin/codecept.js run tests/nfdiv/hwfRefusedByCaseWorker.test.js -c ./ --steps",
    "test:create-sole": "SHOW_BROWSER_WINDOW=yes NODE_PATH=. E2E_AUTO_DELAY_ENABLED=true node ./node_modules/codeceptjs/bin/codecept.js run tests/nfdiv/solicitorCreateSoleDivorceApplication.test.js -c ./ --steps",
    "create:type-definitions": "codeceptjs def",
    "lint": "eslint .",
    "build": "eslint .",
    "test": "eslint .",
    "test:coverage": "eslint .",
    "test:a11y": "eslint .",
    "test-crossbrowser-e2e": "NODE_PATH=. codeceptjs run-multiple ${BROWSER_GROUP:-'--all'} -c saucelabs.conf.js --plugins retryFailedStep --steps --grep '@crossbrowser' --reporter mocha-multi",
    "test:crossbrowser": "./bin/run-crossbrowser-tests.sh"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "pre-commit": "lint",
  "dependencies": {

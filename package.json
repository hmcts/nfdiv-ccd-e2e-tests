{
  "name": "nfdiv-ccd-e2e-tests",
  "version": "0.0.0",
  "private": true,
  "description": "",
  "scripts": {
    "test:test-e2e-standalone": "SHOW_BROWSER_WINDOW=yes NODE_PATH=. E2E_AUTO_DELAY_ENABLED=true node ./node_modules/codeceptjs/bin/codecept.js run tests/nfdiv/solicitorCreateJointApplication.test.js  -c ./ --steps",
    "pretest": "npm run create:type-definitions",
    "test:fullfunctional": "TESTS_FOR_ACCESSIBILITY='true' codeceptjs run-multiple parallel --steps --reporter mochawesome",
    "test:test-general-order": "SHOW_BROWSER_WINDOW=yes NODE_PATH=. E2E_AUTO_DELAY_ENABLED=true node ./node_modules/codeceptjs/bin/codecept.js run tests/nfdiv/generalOrderRefferalAndEmail.test.js  -c ./ --steps",
    "test:test-verify": "SHOW_BROWSER_WINDOW=no NODE_PATH=. E2E_AUTO_DELAY_ENABLED=true node ./node_modules/codeceptjs/bin/codecept.js run tests/nfdiv/verifyCaseStateAndEvents.test.js  -c ./ --steps",
    "test:test-shareACase": "SHOW_BROWSER_WINDOW=yes NODE_PATH=. E2E_AUTO_DELAY_ENABLED=true node ./node_modules/codeceptjs/bin/codecept.js run tests/nfdiv/shareACaseAndMoveToHolding.test.js  -c ./ --steps",
    "test:draft-case": "SHOW_BROWSER_WINDOW=no NODE_PATH=. E2E_AUTO_DELAY_ENABLED=true node ./node_modules/codeceptjs/bin/codecept.js run tests/nfdiv/createDraftCase.test.js -c ./ --steps",
    "create:type-definitions": "codeceptjs def",
    "lint": "eslint .",
    "test-crossbrowser-e2e": "NODE_PATH=. codeceptjs run-multiple ${BROWSER_GROUP:-'--all'} -c saucelabs.conf.js --plugins retryFailedStep --steps --grep '@crossbrowser' --reporter mocha-multi",
    "test:crossbrowser": "./bin/run-crossbrowser-tests.sh"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "pre-commit": "lint",
  "dependencies": {
    "html_codesniffer": "^2.5.1",
    "@hmcts/nodejs-logging": "^3.0.2",
    "cross-env": "^5.2.1",
    "otp": "^0.1.3",
    "request": "^2.88.2",
    "request-promise-native": "^1.0.9"
  },
  "devDependencies": {
    "codeceptjs": "^2.6.11",
    "eslint": "^4.19.1",
    "@typescript-eslint/eslint-plugin": "^4.29.1",
    "@typescript-eslint/parser": "^4.29.1",
    "@babel/core": "^7.15.0",
    "@babel/preset-env": "^7.14.8",
    "babel-eslint": "^10.1.0",
    "babel-loader": "^8.2.2",
    "eslint": "^7.32.0",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-import": "^2.24.0",
    "eslint-plugin-jest": "^24.4.0",
    "eslint-plugin-prettier": "^3.4.0",
    "mocha": "^5.2.0",
    "mocha-jenkins-reporter": "^0.4.5",
    "mocha-junit-reporter": "^1.23.3",
    "mocha-multi": "^1.1.3",
    "mochawesome": "^4.1.0",
    "puppeteer": "^5.4.1",
    "webdriverio": "^6.11.3"
  },
  "resolutions": {
    "axios": ">=0.21.1"
  }
}
